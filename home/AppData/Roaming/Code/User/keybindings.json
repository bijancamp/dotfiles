// Place your key bindings in this file to override the defaults
[
    {
        "key": "shift+alt+q",
        "command": "rewrap.rewrapComment",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+q",
        "command": "-rewrap.rewrapComment",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+l",
        "command": "lazygit-vscode.toggle"
    },
    {
        "key": "ctrl+shift+l",
        "command": "-lazygit-vscode.toggle"
    }
]
